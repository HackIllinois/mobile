{
  "expo": {
    "scheme": "hackillinois",
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yaseenhalabi.mobile",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "For peer to peer connection for local gameplay",
        "NSBonjourServices": [
          "_shooter-game._tcp",
          "_shooter-game._udp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yaseenhalabi.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8d4de9fa-ad6a-4137-93b2-ea7a0f666007"
      }
    }
  }
}
